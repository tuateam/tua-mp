{
    "name": "@tua-mp/cli",
    "version": "0.2.1",
    "description": "tua-mp 命令行工具",
    "bin": {
        "tuamp": "bin/tuamp"
    },
    "scripts": {
        "cov": "open coverage/lcov-report/index.html",
        "test": "cross-env NODE_ENV=test jest && codecov",
        "test:tdd": "cross-env NODE_ENV=test jest --watch",
        "lint": "eslint --fix lib/ bin/ test/",
        "clean": "rimraf src && mkdir src/ src/app/ src/apis/ src/pages/ src/comps/ && echo {} > src/app/app.json",
        "precommit": "lint-staged",
        "pub": "npm test && npm publish"
    },
    "lint-staged": {
        "bin/*": [
            "eslint --fix",
            "git add"
        ],
        "{lib,test}/**/*.js": [
            "eslint --fix",
            "git add"
        ]
    },
    "jest": {
        "bail": true,
        "collectCoverage": true,
        "testEnvironment": "node",
        "collectCoverageFrom": [
            "lib/**"
        ]
    },
    "eslintIgnore": [
        "package.json"
    ],
    "dependencies": {
        "@vue/cli-init": "^3.4.1",
        "babel-preset-env": "^1.7.0",
        "chalk": "^2.4.2",
        "handlebars": "^4.1.0",
        "inquirer": "^6.2.2",
        "inquirer-fuzzy-path": "^2.0.2",
        "log-symbols": "^2.2.0",
        "semver": "^5.6.0",
        "treeify": "^1.1.0",
        "yargs": "^13.2.1"
    },
    "devDependencies": {
        "codecov": "^3.2.0",
        "cross-env": "^5.2.0",
        "eslint": "^5.15.1",
        "jest": "^24.1.0",
        "rimraf": "^2.6.3"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tuateam/tua-mp.git"
    },
    "keywords": [
        "cli",
        "tua-mp",
        "tua-mp-cli",
        "@tua-mp/cli"
    ],
    "author": "StEve Young",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/tuateam/tua-mp/issues"
    },
    "homepage": "https://github.com/tuateam/tua-mp#readme",
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">=8"
    }
}
